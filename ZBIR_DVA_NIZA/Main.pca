		adrA = 1
		adrB = 2
		adrC = 3
		N = 4
		I = 5
		A = 100
		B = 200
		C = 300
		
		ORG 8
		
		IN N
		mov adrA, #A
		mov adrB, #B
		mov adrC, #C
		IN (adrA), N
		IN (adrB), N
		mov I, 0
		
		loop: BEQ N, I, kraj
		add (adrC), (adrA), (adrB)
		
		add adrA, adrA, 1
		add adrB, adrB, 1
		add adrC, adrC, 1
		add I, I, 1
		
		BEQ I, I, loop
		
		kraj: mov adrC, #C
		OUT (adrC), N
		STOP
		
		
		
		
		
